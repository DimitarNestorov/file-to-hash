{
	"name": "file-to-hash",
	"version": "0.0.1",
	"description": "Calculates a given hash of a given file. Single package for the browser and node.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "git@github.com:dimitarnestorov/file-to-hash.git",
	"homepage": "https://github.com/dimitarnestorov/file-to-hash/tree/master/packages/file-to-hash",
	"author": "Dimitar Nestorov <opensource@dimitarnestorov.com>",
	"license": "MIT",
	"keywords": [
		"blob",
		"file",
		"path",
		"data",
		"string",
		"buffer",
		"checksum",
		"hash",
		"hex",
		"base64",
		"crypto"
	],
	"scripts": {
		"prepublishOnly": "yarn run build",
		"build": "rimraf dist && tsc",
		"watch": "rimraf dist && tsc -w"
